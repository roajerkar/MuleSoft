  <xs:element xmlns="http://www.mulesoft.org/schema/mule/splunk" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="Get-jobsType" substitutionGroup="mule:abstract-message-processor" name="get-jobs">
